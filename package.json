{
  "name": "angular7",
  "version": "1.0.0",
  "description": "AoT built angular app",
  "main": "src/app/main.ts",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "start_aot": "webpack-dev-server --env.aot --inline --progress --port 8080",
    "build": "rimraf dist && webpack --env.aot --env.prod --progress --bail",
    "build-aot": "rimraf dist && webpack --config webpack.aot.config.js --progress --bail",
    "build-jit": "rimraf dist && webpack --config webpack.aot.config.js --progress --bail",
    "i18n": "ng-xi18n -p tsconfig.i18n.json && shx mv ./app/ngfactory/messages.xlf ./app/locale/messages.xlf && rimraf temp && rimraf app/ngfactory"
  },
  "author": "Emmanuel Antaya",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.4.7",
    "@angular/compiler": "^2.4.7",
    "@angular/compiler-cli": "^2.4.7",
    "@angular/core": "^2.4.7",
    "@angular/forms": "^2.4.7",
    "@angular/http": "^2.4.7",
    "@angular/platform-browser": "^2.4.7",
    "@angular/platform-browser-dynamic": "^2.4.7",
    "@angular/platform-server": "^2.4.7",
    "@angular/router": "^3.4.7",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "rxjs": "^5.1.1",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.2.9",
    "@types/node": "^7.0.5",
    "angular-router-loader": "^0.5.0",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.7.3",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "postcss-loader": "^1.3.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "shx": "^0.2.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.4.2",
    "typescript": "^2.1.6",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
