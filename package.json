{
  "name": "angular7",
  "version": "1.0.0",
  "description": "AoT built angular app",
  "main": "src/app/main.ts",
  "scripts": {
    "start": "npm run build && webpack-dev-server --env.aot --env.prod --inline --progress --port 8080",
    "start_en": "webpack-dev-server --env.aot --env.prod --inline --progress --port 8080",
    "debug": "npm run build_debug && webpack-dev-server --inline --progress --port 8080",
    "debug_en": "webpack-dev-server --inline --progress --port 8080",
    "build": "rimraf dist && webpack --env.aot --env.prod --progress --bail  && webpack --env.aot --env.prod --env.locale=fr --progress --bail  && webpack --env.aot --env.prod --env.locale=es --progress --bail",
    "build_debug": "rimraf dist && webpack --progress --bail  && webpack --env.locale=fr --progress --bail  && webpack --env.locale=es --progress --bail",
    "i18n": "ng-xi18n -p tsconfig.i18n.json && shx mv ./app/ngfactory/messages.xlf ./app/locale/messages.xlf && rimraf temp && rimraf app/ngfactory"
  },
  "author": "Emmanuel Antaya",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^4.0.2",
    "@angular/common": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/compiler-cli": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/material": "^2.0.0-beta.3",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@angular/platform-server": "^4.0.2",
    "@angular/router": "^4.0.2",
    "core-js": "^2.4.1",
    "d3": "^4.8.0",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.3.0",
    "@types/node": "^7.0.12",
    "@types/d3": "^4.7.0",
    "angular-router-loader": "^0.6.0",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.7.7",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "shx": "^0.2.2",
    "style-loader": "^0.16.1",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^2.0.3",
    "tslint": "^5.1.0",
    "tslint-loader": "^3.5.2",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
